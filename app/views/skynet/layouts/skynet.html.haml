!!!
/[if IE 8] <html class="no-js lt-ie9">
/ [if gt IE 8] 
%html.no-js
  / <![endif]
  %head
    %title Skynet
    %meta{charset: "UTF-8"}
    %meta{name: 'viewport', content: 'width=device-width, initial-scale=1.0, maximum-scale=1.0, user-scalable=yes'}

    = stylesheet_link_tag    'skynet/application', media: 'all', 'data-turbolinks-track' => true
    = javascript_include_tag 'skynet/application', 'data-turbolinks-track' => true
    = csrf_meta_tags
  %body
    %nav.sidebar
      %header
        %span.glyphicon.glyphicon-flash
        Gemgento
      %ul.nav.nav-pills.nav-stacked
        %li
          %a{href: "/skynet/"}
            %span.glyphicon.glyphicon-globe
            Overview
        %li
          %a{href: "/skynet/products"}
            %span.glyphicon.glyphicon-tag
            Products
        %li
          %a{href: "/skynet/categories"}
            %span.glyphicon.glyphicon-folder-open
            Categories
        %li
          %a{href: "/skynet/pages"}
            %span.glyphicon.glyphicon-book
            Pages
        %li
          %a{href: "/skynet/lookbooks"}
            %span.glyphicon.glyphicon-cog
            Lookbooks
        %li 
          %a{href: "/skynet/stores"}
            %span.glyphicon.glyphicon-home
            Store Locator
        %li     
          %a{href: "/skynet/settings"}
            %span.glyphicon.glyphicon-wrench
            Settings
    %main.main#main
      = yield






